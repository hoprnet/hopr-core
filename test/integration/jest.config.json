{
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "maxConcurrency": 1,
    "reporters": [
      [
        "jest-silent-reporter",
        {
          "useDots": true
        }
      ]
    ]
  }
