{
  "name": "@hoprnet/hopr-protos",
  "version": "0.0.1-7c61321",
  "main": "lib/index.js",
  "files": [
    "lib/",
    "src/**/*.proto"
  ],
  "license": "GPL-3.0-only",
  "scripts": {
    "build": "yarn clean && yarn build:protos && tsc",
    "build:protos": "./node_modules/protoc/protoc/bin/protoc --plugin=node_modules/ts-proto/protoc-gen-ts_proto --ts_proto_out=./ src/*.proto",
    "clean": "rimraf lib",
    "test": "echo '⚠️ No tests implemented'",
    "prepublishOnly": "yarn build"
  },
  "engines": {
    "node": "12.9.1",
    "yarn": "1.19.2"
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "devDependencies": {
    "@types/node": "^12.11.0",
    "protoc": "^1.0.4",
    "rimraf": "^3.0.2",
    "ts-proto": "^1.25.0",
    "typescript": "^3.9.6"
  }
}
